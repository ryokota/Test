            <apex:inputField id="SFO_lname_kana" value="{!timeCard.lname_kana_Text__c}" />
        </apex:outputPanel>

    <apex:pageBlock Title="検索結果" id="Select_Error">
        <apex:pageBlockButtons Location="top" style="align:left;">
            <apex:commandButton alt="一覧表示" value="一覧表示" action="{!doSearch}"/>
            <apex:commandButton value="保存" id="SaveButton" onclick="getTable('{!$Component.Select_Error}')" action="{!save}"/>
            <!--<apex:commandButton value="時間計算" id="CalcButton" action="{!calculate}"/> -->
        </apex:pageBlockButtons>
        <apex:pageBlockSection >
            <apex:outputPanel layout="block" style="overflow:auto;position:relative;" id="TimaCardTable">
                <table cellspacing="0" cellpadding="4" rules="all" id="TimeCardGrid" bordercolor="#3366CC" border="1" style="overflow:auto;background-color:White;border-width:1px;border-style:double;width:1250px;border-collapse:collapse;">
                    <thead>
                        <tr style="color:#000000;background-color:#E5F1FF;font-weight:bold;">
                            <td>年月日
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectSyukkinDateASC}" style="display:{!SyukkinDateASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectSyukkinDateDESC}" style="display:{!SyukkinDateDESC}"/>
                            </td>
                            <td>曜日</td>
                            <td>氏名</td>
                            <td>氏名カナ
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectlnamekanaASC}" style="display:{!lnamekanaASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectlnamekanaDESC}" style="display:{!lnamekanaDESC}"/>
                            </td>

                            <td>SCD
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectSCDASC}" style="display:{!SCDASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectSCDDESC}" style="display:{!SCDDESC}"/>
                            </td>
                            <td>現在状況
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectStaffStatusASC}" style="display:{!staffStatusASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectStaffStatusDESC}" style="display:{!staffStatusDESC}"/>
                            </td>
                            <td>訓練期間
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectTrainingTermASC}" style="display:{!trainingTermASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectTrainingTermDESC}" style="display:{!trainingTermDESC}"/>
                            </td>
                            <td>コース</td>
                            <td>リーダー
                               <apex:commandButton alt="▲" value="▲" action="{!doSelectleaderASC}" style="display:{!leaderASC}"/>
                               <apex:commandButton alt="▼" value="▼" action="{!doSelectleaderDESC}" style="display:{!leaderDESC}"/>
                            </td>

                            <td>勤怠区分</td>
                            <td>開始時間</td>
                            <td>終了時間</td>
                            <td>休憩時間</td>
                            <td>深夜休憩時間</td>
                            <td>勤務時間</td>
                            <td>うち時間外勤務</td>
                            <td>うち深夜勤務</td>
                            <td>予定勤務時間</td>
                            <td style="display: none;">時間単価（非表示）</td>
                        </tr>
                    </thead>
                    <tbody>
                        <apex:repeat value="{!tcwList}" var="v_TimeCard" id="TimeCardRepeat">
                            <tr style="border:solid 1px #3366CC;background-color:White;">
                                <td style="width:70px">
                                    <apex:outputField id="nengetubi" value="{!v_TimeCard.timeCard.Syukkin_Date__c}"/>
                                </td>
                                <td>
                                    <apex:outputLabel id="youbi" value="{!v_TimeCard.youbi}" style="color:{!v_TimeCard.youbiColor};"/>
                                </td>
                                <td style="width:110px">
                                    <apex:outputField id="shimei" value="{!v_TimeCard.timeCard.staff__r.Name}" />
                                </td>

                                <td style="width:110px">
                                    <apex:outputField id="shimeiKana" value="{!v_TimeCard.timeCard.lname_kana__c}" />
                                </td>

                                <td style="width:50px">
                                    <apex:outputField id="SCD" value="{!v_TimeCard.timeCard.staff__r.purestaffcode__c}"/>
                                </td>
                                <td style="width:30px">
                                   <apex:outputField id="genzai_joukyou" value="{!v_TimeCard.timeCard.Kbn__c}"/>
                                </td>
                                <td style="width:40px">
                                    <apex:outputField id="kunren_kikan" value="{!v_TimeCard.timeCard.staff__r.trainingterm__c}"/>
                                </td>
                                <td style="width:50px">
                                   <apex:outputField id="team" value="{!v_TimeCard.timeCard.staff__r.team__r.Name}"/>
                                </td>

                                <td style="width:50px">
                                   <apex:outputField id="leader" value="{!v_TimeCard.timeCard.leader__c}"/>
                                </td>

                                <td>
                                    <apex:inputField id="kintai_kubun" value="{!v_TimeCard.timeCard.workrecord__c}" onchange="disableSelect(2,'{!$Component.kintai_kubun}','{!$Component.syussya_Time_Hours}','{!$Component.syussya_Time_Minutes}',
                                                                                                                                            '{!$Component.taisya_Time_Hours}','{!$Component.taisya_Time_Minutes}','{!$Component.rest_Time_Hours}',
                                                                                                                                            '{!$Component.rest_Time_Minutes}','{!$Component.midnight_Rest_Time_Hours}','{!$Component.midnight_Rest_Time_Minutes}',
                                                                                                                                            '{!$Component.working_Time}','{!$Component.over_Time}','{!$Component.midnight_Work_Time}',
