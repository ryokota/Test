<apex:page Controller="TimeCardCheckController" sidebar="false">
    <head>
        <style type="text/css">
            .requiredInput {
                display: inline !important;
            }
            .requiredBlock {
                display: inline !important;
            }
        </style>
        <script type="text/javascript">
            beenFocused = true;
        </script>
        <script type="text/javascript">
            function disableSelect(n,objKintaiKubunId,objSyussyaTimeHoursId,objSyussyaTimeMinutesId,objTaisyaTimeHoursId,objTaisyaTimeMinutesId,
                       objRestTimeHoursId,objRestTimeMinutesId,objMidnightRestTimeHoursId,objMidnightRestTimeMinutesId,objWorkingTimeId,
                       objOverTimeId,objMidnightWorkTimeId,objScheduledWorkingHoursId,objJikanTanka,objSalaryId,objYoubi,objLeader,
                       objShimeiKana,objgenzaijoukyou,objfixedworktime,objNengetubi){

				//予定勤務時間を初期化
				document.getElementById(objScheduledWorkingHoursId).value = 0;

				//初期読込み時・・・現在状況が「OJT」または「OFFJT」または「退職など」の状態でまだ退職当日以前の場合は
				//                  勤怠区分が「--なし--」の場合、「出勤」に変更
				//上記以外の場合・・・勤怠区分を全て「--なし--」に変更
                if(n != 0){
					//今日を設定
					var today = new Date();
					//時間を0:00にする
					today.setHours(0);
					today.setMinutes(0);
					today.setSeconds(0);
					today.setMilliseconds(0);
					
					var nengetuDateTime = new Date(objNengetubi);
					
                	if(objgenzaijoukyou != 'OffJT' && objgenzaijoukyou != 'OJT'){

                		document.getElementById(objKintaiKubunId).disabled = false;
                        document.getElementById(objSyussyaTimeHoursId).disabled = true;
                        document.getElementById(objSyussyaTimeMinutesId).disabled = true;
                        document.getElementById(objTaisyaTimeHoursId).disabled = true;
                        document.getElementById(objTaisyaTimeMinutesId).disabled = true;
                        document.getElementById(objRestTimeHoursId).disabled = true;
                        document.getElementById(objRestTimeMinutesId).disabled = true;
                        document.getElementById(objMidnightRestTimeHoursId).disabled = true;
                        document.getElementById(objMidnightRestTimeMinutesId).disabled = true;

                        document.getElementById(objKintaiKubunId).value = '';

                        document.getElementById(objSyussyaTimeHoursId).value = '00';
                        document.getElementById(objSyussyaTimeMinutesId).value = '00';
                        document.getElementById(objTaisyaTimeHoursId).value = '00';
                        document.getElementById(objTaisyaTimeMinutesId).value = '00';
                        document.getElementById(objRestTimeHoursId).value = '00';
                        document.getElementById(objRestTimeMinutesId).value = '00';
                        document.getElementById(objMidnightRestTimeHoursId).value = '00';
                        document.getElementById(objMidnightRestTimeMinutesId).value = '00';

                    /*勤怠区分が「出勤」,「--なし--」※初期読込み限定
                    出社時間(時),出社時間(分),退社時間(時),(退社時間(分),休憩時間(時),休憩時間(分),深夜休憩時間(時),深夜休憩時間(分)・・・全て押下可
                    出社時間(時),出社時間(分)が00:00の場合・・・09:30を設定
                    退社時間(時),(退社時間(分)が00:00の場合・・・17:30を設定
                    休憩時間(時),休憩時間(分)が00:00の場合・・・01:00を設定
                    */
                    } else if(document.getElementById(objKintaiKubunId).value == '出勤' || ((n == 2 && document.getElementById(objKintaiKubunId).value == null) &&
                       objYoubi.style.color != 'red' && objYoubi.style.color != 'blue')){

						//勤怠区分を出勤に変更
						document.getElementById(objKintaiKubunId).value = '出勤';

                        //今日より日付が後で祝日を除く平日のときに予定勤務時間を入力
                        if(today.getTime() <= nengetuDateTime.getTime()){

							//リーダーの場合は予定勤務時間に8.0を入力
							if(objLeader == 'L'){

								//予定勤務時間に8.0を入力
                        		document.getElementById(objScheduledWorkingHoursId).value = 8.0;

                        	} else {

								//予定勤務時間に7.5を入力
                        		document.getElementById(objScheduledWorkingHoursId).value = 7.5;

                        	}

                       }

                        document.getElementById(objSyussyaTimeHoursId).disabled = false;
                        document.getElementById(objSyussyaTimeMinutesId).disabled = false;
                        document.getElementById(objTaisyaTimeHoursId).disabled = false;
                        document.getElementById(objTaisyaTimeMinutesId).disabled = false;
                        document.getElementById(objRestTimeHoursId).disabled = false;
                        document.getElementById(objRestTimeMinutesId).disabled = false;
                        document.getElementById(objMidnightRestTimeHoursId).disabled = false;
                        document.getElementById(objMidnightRestTimeMinutesId).disabled = false;

                        if(document.getElementById(objSyussyaTimeHoursId).value == '00' && document.getElementById(objSyussyaTimeMinutesId).value == '00' &&
                           document.getElementById(objKintaiKubunId).value == '出勤' && objfixedworktime == 0){

                            document.getElementById(objSyussyaTimeHoursId).value = '09';
                            document.getElementById(objSyussyaTimeMinutesId).value = '00';

                        }

                        if(document.getElementById(objTaisyaTimeHoursId).value == '00' && document.getElementById(objTaisyaTimeMinutesId).value == '00' &&
                           document.getElementById(objKintaiKubunId).value == '出勤' && objfixedworktime == 0){

                        	//リーダーの場合は勤務終了時間に8.0を入力
							if(objLeader == 'L'){
                            	document.getElementById(objTaisyaTimeHoursId).value = '18';
                            	document.getElementById(objTaisyaTimeMinutesId).value = '00';
                            } else {
                            	document.getElementById(objTaisyaTimeHoursId).value = '17';
                            	document.getElementById(objTaisyaTimeMinutesId).value = '30';
                            }

                        }

                        if(document.getElementById(objRestTimeHoursId).value == '00' && document.getElementById(objRestTimeMinutesId).value == '00' &&
                           document.getElementById(objKintaiKubunId).value == '出勤' && objfixedworktime == 0){

                            document.getElementById(objRestTimeHoursId).value = '01';
                            document.getElementById(objRestTimeMinutesId).value = '00';

                        }

                    /*有休の場合
                    出社時間(時),出社時間(分),退社時間(時),(退社時間(分),休憩時間(時),休憩時間(分),深夜休憩時間(時),深夜休憩時間(分)・・・全て押下不可
                    出社時間(時),出社時間(分)・・・09:30を設定
                    退社時間(時),(退社時間(分)・・・17:30を設定
                    休憩時間(時),休憩時間(分)・・・01:00を設定
                    深夜休憩時間(時),深夜休憩時間(分)・・・00:00を設定
                    */
                    } else if(document.getElementById(objKintaiKubunId).value == '有休'){

                        document.getElementById(objSyussyaTimeHoursId).disabled = true;
                        document.getElementById(objSyussyaTimeMinutesId).disabled = true;
                        document.getElementById(objTaisyaTimeHoursId).disabled = true;
                        document.getElementById(objTaisyaTimeMinutesId).disabled = true;
                        document.getElementById(objRestTimeHoursId).disabled = true;
                        document.getElementById(objRestTimeMinutesId).disabled = true;
                        document.getElementById(objMidnightRestTimeHoursId).disabled = true;
                        document.getElementById(objMidnightRestTimeMinutesId).disabled = true;
                        document.getElementById(objSyussyaTimeHoursId).value = '09';
                        document.getElementById(objSyussyaTimeMinutesId).value = '00';

                        //リーダーの場合は勤務終了時間に8.0を入力
						if(objLeader == 'L'){
                           	document.getElementById(objTaisyaTimeHoursId).value = '18';
                           	document.getElementById(objTaisyaTimeMinutesId).value = '00';
                        } else {
                           	document.getElementById(objTaisyaTimeHoursId).value = '17';
                           	document.getElementById(objTaisyaTimeMinutesId).value = '30';
                        }

                        document.getElementById(objRestTimeHoursId).value = '01';
                        document.getElementById(objRestTimeMinutesId).value = '00';
                        document.getElementById(objMidnightRestTimeHoursId).value = '00';
                        document.getElementById(objMidnightRestTimeMinutesId).value = '00';

                    /*欠勤の場合
                    出社時間(時),出社時間(分),退社時間(時),(退社時間(分),休憩時間(時),休憩時間(分),深夜休憩時間(時),深夜休憩時間(分)・・・全て押下不可
                    出社時間(時),出社時間(分)・・・00:00を設定
                    退社時間(時),(退社時間(分)・・・00:00を設定
                    休憩時間(時),休憩時間(分)・・・00:00を設定
                    深夜休憩時間(時),深夜休憩時間(分)・・・00:00を設定
                    */
                    } else {

                        document.getElementById(objSyussyaTimeHoursId).disabled = true;
                        document.getElementById(objSyussyaTimeMinutesId).disabled = true;
                        document.getElementById(objTaisyaTimeHoursId).disabled = true;
                        document.getElementById(objTaisyaTimeMinutesId).disabled = true;
                        document.getElementById(objRestTimeHoursId).disabled = true;
                        document.getElementById(objRestTimeMinutesId).disabled = true;
                        document.getElementById(objMidnightRestTimeHoursId).disabled = true;
                        document.getElementById(objMidnightRestTimeMinutesId).disabled = true;
                        document.getElementById(objSyussyaTimeHoursId).value = '00';
                        document.getElementById(objSyussyaTimeMinutesId).value = '00';
                        document.getElementById(objTaisyaTimeHoursId).value = '00';
                        document.getElementById(objTaisyaTimeMinutesId).value = '00';
                        document.getElementById(objRestTimeHoursId).value = '00';
                        document.getElementById(objRestTimeMinutesId).value = '00';
                        document.getElementById(objMidnightRestTimeHoursId).value = '00';
                        document.getElementById(objMidnightRestTimeMinutesId).value = '00';

                    }

                }

                /***************************************************************************************************************************************************
                勤務時間の計算
                退社時間(時),退社時間(分)の合計時間が出社時間(時),出社時間(分) + 休憩時間(時),休憩時間(分) + 深夜休憩時間(時),深夜休憩時間(分)よりも値が小さい場合は
                勤務時間,残業時間,深夜残業時間,日給を全て0に設定
                退社時間(時),退社時間(分)の合計時間が出社時間(時),出社時間(分) + 休憩時間(時),休憩時間(分) + 深夜休憩時間(時),深夜休憩時間(分)よりも値が大きい場合は
                合計勤務時間,残業時間,日給の計算をする
                ・合計勤務時間の計算 = 退社時間(時) + (退社時間(分) / 60) - (出社時間(時) + 出社時間(分) + (休憩時間(時) + (休憩時間(分) / 60)) + (深夜休憩時間(時) + (深夜休憩時間(分) / 60))
                合計勤務時間が7.5h(リーダーは8.0h)を超える場合は残業時間の計算をする
                ・合計残業時間(訓練生) = 合計勤務時間 - 7.5
                ・合計残業時間(リーダー) = 合計勤務時間 - 8.0
                退社時間(時),退社時間(分)が22時を超える場合は深夜残業時間の計算
                ・合計深夜残業時間 = 退社時間(時) + (退社時間(分) / 60 ) - 22
                ・給料計算(平日) = 合計勤務時間 * 時間単価 + (合計残業時間 * (時間単価 * 0.25)) + (合計深夜残業時間 * (時間単価 * 0.25))
                ・給料計算(土日祝) = 合計勤務時間 * (時間単価 * 1.25) + (合計残業時間 * (時間単価 * 0.25)) + (合計深夜残業時間 * (時間単価 * 0.25))
                ※計算結果は小数第三位以下四捨五入
                ****************************************************************************************************************************************************/
                if(Number(document.getElementById(objTaisyaTimeHoursId).value) + (Number(document.getElementById(objTaisyaTimeMinutesId).value) / 60) >
                   Number(document.getElementById(objSyussyaTimeHoursId).value) + (Number(document.getElementById(objSyussyaTimeMinutesId).value) / 60) +
                   Number(document.getElementById(objRestTimeHoursId).value) + (Number(document.getElementById(objRestTimeMinutesId).value) / 60)){

                    document.getElementById(objWorkingTimeId).value =  Math.round((Number(document.getElementById(objTaisyaTimeHoursId).value) +
                                                                    (Number(document.getElementById(objTaisyaTimeMinutesId).value) / 60) -
                                                                    (Number(document.getElementById(objSyussyaTimeHoursId).value) +
                                                                    (Number(document.getElementById(objSyussyaTimeMinutesId).value) / 60) +
                                                                     Number(document.getElementById(objRestTimeHoursId).value) +
                                                                    (Number(document.getElementById(objRestTimeMinutesId).value) / 60) +
                                                                     Number(document.getElementById(objMidnightRestTimeHoursId).value) +
                                                                    (Number(document.getElementById(objMidnightRestTimeMinutesId).value) / 60))) * 100) / 100;

					//時間外勤務の計算
                    if(Number(document.getElementById(objWorkingTimeId).value) > 7.5 && objLeader != 'L'){

                        document.getElementById(objOverTimeId).value = Math.round((Number(document.getElementById(objWorkingTimeId).value) - 7.5) * 100) / 100;

                    } else if(Number(document.getElementById(objWorkingTimeId).value) > 8.0 && objLeader == 'L'){

                        document.getElementById(objOverTimeId).value = Math.round((Number(document.getElementById(objWorkingTimeId).value) - 8.0) * 100) / 100;

                    } else {

                    	document.getElementById(objOverTimeId).value = 0;

                    }

                    //深夜残業時間の計算 = 退社時間(時) + (退社時間(分) / 60) - (22時 + (深夜休憩時間(時) + (深夜休憩時間(分) / 60)))
                    if(Number(document.getElementById(objTaisyaTimeHoursId).value) + (Number(document.getElementById(objTaisyaTimeMinutesId).value) / 60) > 22 +
                      (Number(document.getElementById(objMidnightRestTimeHoursId).value) + (Number(document.getElementById(objMidnightRestTimeMinutesId).value) / 60))){

                        document.getElementById(objMidnightWorkTimeId).value =  Math.round((Number(document.getElementById(objTaisyaTimeHoursId).value) +
                                                                               (Number(document.getElementById(objTaisyaTimeMinutesId).value) / 60) - (22 +
                       															Number(document.getElementById(objMidnightRestTimeHoursId).value) +
                       														   (Number(document.getElementById(objMidnightRestTimeMinutesId).value) / 60))) * 100) / 100;

                    } else {

                    	document.getElementById(objMidnightWorkTimeId).value = 0;

                    }

                    //休日の場合の給料計算
                    if(objYoubi.style.color == 'red' || objYoubi.style.color == 'blue'){

                        document.getElementById(objSalaryId).value = Math.round(Number(document.getElementById(objWorkingTimeId).value) *
                                                                     Number(objJikanTanka) * 1.25 +
                                                                    (Number(document.getElementById(objOverTimeId).value) *
                                                                    (Number(objJikanTanka) * 0.25)) +
                                                                    (Number(document.getElementById(objMidnightWorkTimeId).value) *
                                                                    (Number(objJikanTanka) * 0.25)));
                    //平日の場合の給料計算
                    } else {

                        document.getElementById(objSalaryId).value = Math.round(Number(document.getElementById(objWorkingTimeId).value) *
                                                                     Number(objJikanTanka) +
                                                                    (Number(document.getElementById(objOverTimeId).value) *
                                                                    (Number(objJikanTanka) * 0.25)) +
                                                                    (Number(document.getElementById(objMidnightWorkTimeId).value) *
                                                                    (Number(objJikanTanka) * 0.25)));
                    }

                } else {

                    document.getElementById(objWorkingTimeId).value = 0;
                    document.getElementById(objOverTimeId).value = 0;
                    document.getElementById(objMidnightWorkTimeId).value = 0;
                    document.getElementById(objSalaryId).value = 0;
                }
            }

            function getTable(objTableId){

            	var objTable = document.getElementById(objTableId);
            	clearDisabled(objTable, 'input');
            	clearDisabled(objTable, 'select');

            }

			function clearDisabled(src, tagName) {
				var nodes = src.getElementsByTagName(tagName);
				var i = 0;
				var len = nodes.length;

				for (; i < len; i++) {
					nodes[i].disabled = false;
				}
			}

        </script>
    </head>
    <apex:form >
        <apex:messages style="color:red;font-size: 30px;" />
        <apex:outputPanel layout="block">
            <apex:outputLabel for="Syukkin_Date__c">年月日</apex:outputLabel>
            <apex:inputField id="Syukkin_Date_From__c" value="{!timeCard.Syukkin_Date__c}"/>
            <apex:outputLabel for="Syukkin_Date__c">～</apex:outputLabel>
            <apex:inputField id="Syukkin_Date_To__c" value="{!timeCard.Syukkin_Date_To__c}"/>
        </apex:outputPanel>
        <apex:outputPanel layout="block">
            <apex:outputLabel for="SFOtrainingterm">訓練期間</apex:outputLabel>
            <apex:inputField id="SFOtrainingterm" value="{!timeCard.trainingtermSearch__c}"/>
            <apex:outputLabel for="teamSelect">コース</apex:outputLabel>
            <apex:selectList id="teamSelect" value="{!teamValue}" size="1" multiselect="false">
                <apex:selectOptions value="{!SFOteamList}"/>
            </apex:selectList>
        </apex:outputPanel>

        <apex:outputPanel layout="block" >
            <apex:outputLabel for="Name">訓練生</apex:outputLabel>
            <apex:inputField id="SFOstaff" value="{!timeCard.staff_Search__c}" />
