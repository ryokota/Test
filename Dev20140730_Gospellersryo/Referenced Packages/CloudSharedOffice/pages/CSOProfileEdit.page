        });
    $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").change(function(e) {
        linkedCheckbox("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_STANDARD\\:SPACEUSER_EXPORT_STANDARD", $(this).attr("checked"));
        });

    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_STANDARD\\:COLLABO_SPACE_STANDARD").change(function(e) {
        if(!$(this).attr("checked") && !$("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_STANDARD\\:COMPANY_SPACE_STANDARD").attr("checked")){
            if($("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").attr("checked")){
