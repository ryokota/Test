            }
            $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_STANDARD\\:SPACEUSER_EXPORT_STANDARD").removeAttr("checked");
            $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").attr("disabled", "disabled");
            $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_STANDARD\\:SPACEUSER_EXPORT_STANDARD").attr("disabled", "disabled");
        } else if ($(this).attr("checked") && !$("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").attr("checked")){
            $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").removeAttr("disabled");
            $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_STANDARD\\:SPACEUSER_EXPORT_STANDARD").removeAttr("disabled");
        }
    });
    // コンテンツ一覧のカスタムが有効になると、コンテンツとスペースのカスタム項目をONに
    $("#" + preId + "\\:theBlockSectionCustomColumns\\:theBlockSectionItemLIST_SETTING_CONTENTS\\:LIST_SETTING_CONTENTS").change(function(e) {
        if($(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_CONTENTS\\:CUSTOM_FIELDS_CONTENTS").attr("checked", "checked");
            $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_SPACE\\:CUSTOM_FIELDS_SPACE").attr("checked", "checked");
        }
    });
    // コンテンツのカスタム項目がOFFになったら、コンテンツ一覧のカスタムをOFFに
    $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_CONTENTS\\:CUSTOM_FIELDS_CONTENTS").change(function(e){
        if (!$(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionCustomColumns\\:theBlockSectionItemLIST_SETTING_CONTENTS\\:LIST_SETTING_CONTENTS").removeAttr("checked");
        }
    });
    // スペースのカスタム項目がOFFになったら、コンテンツ一覧のカスタムをOFFに
    $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_SPACE\\:CUSTOM_FIELDS_SPACE").change(function(e){
        if (!$(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionCustomColumns\\:theBlockSectionItemLIST_SETTING_CONTENTS\\:LIST_SETTING_CONTENTS").removeAttr("checked");
        }
    });

    /* 初期状態を反映する */
    $("#" + preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT").keyup();
    $("#" + preId + "\\:theBlockSectionEvidence\\:theBlockSectionItemEVIDENCE_CSV_EXPORT\\:EVIDENCE_CSV_EXPORT").change();
    $("#" + preId + "\\:theBlockSectionEvidence\\:theBlockSectionItemEVIDENCE_MANAGE\\:EVIDENCE_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionDelivery\\:theBlockSectionItemDELIVERY_MANAGE\\:DELIVERY_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD").change();
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_SETTINGS\\:WORKFLOW_SETTINGS").change();
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_DEFINITION\\:WORKFLOW_DEFINITION").change();
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_CATEGORY\\:WORKFLOW_CATEGORY").change();
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_MANAGE\\:WORKFLOW_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_MANAGE\\:COLLABO_SPACE_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_ROOT_DEFINE\\:COLLABO_SPACE_ROOT_DEFINE").change();
    $("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_MANAGE\\:COMPANY_SPACE_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_ROOT_DEFINE\\:COMPANY_SPACE_ROOT_DEFINE").change();
    $("#" + preId + "\\:theBlockSectionArchive\\:theBlockSectionItemARCHIVE_SPACE_MANAGE\\:ARCHIVE_SPACE_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionSupport\\:theBlockSectionItemSUPPORT_MANAGE\\:SUPPORT_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionUserManage\\:theBlockSectionItemUSER_USER_MANAGE\\:USER_USER_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionSpaceUserExport\\:theBlockSectionItemSPACEUSER_EXPORT_MANAGE\\:SPACEUSER_EXPORT_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_STANDARD\\:COLLABO_SPACE_STANDARD").change();
    $("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_STANDARD\\:COMPANY_SPACE_STANDARD").change();
    $("#" + preId + "\\:theBlockSectionSupport\\:theBlockSectionItemSYNC_MANAGE\\:SYNC_MANAGE").change();
    $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_CONTENTS\\:CUSTOM_FIELDS_CONTENTS").change();
    $("#" + preId + "\\:theBlockSectionCustomFields\\:theBlockSectionItemCUSTOM_FIELDS_SPACE\\:CUSTOM_FIELDS_SPACE").change();
    $("#" + preId + "\\:theBlockSectionCustomColumns\\:theBlockSectionItemLIST_SETTING_CONTENTS\\:LIST_SETTING_CONTENTS").change();

}

/* 指定のチェックボックスを変更する */
function linkedCheckbox(id, isOn){
    if(isOn){
        $(id).attr("checked", "checked");
        $(id).attr("disabled", "disabled");
    }else{
        $(id).removeAttr("disabled");
    }
    $(id).change();
}

/* 保存前にdisabledを外す */
function removeAttrDis(){
    var inputTags  =   document.getElementsByTagName('input');

    CsoScript_loadingStart();

    for(var i = 0; i < inputTags.length ; i++){
        if (inputTags[i].type == 'checkbox') {
            inputTags[i].disabled = false;
        }
    }

   return true;
}

