<script type="text/javascript">

var preId = "CSOProfileEditPage\\:theForm\\:theBlock";

$(function(){
    connectView();
});

/* 画面と連動する */
function connectView(){
    /* コメント文字数をチェックする */
    $("#" + preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT").keyup(function(e){
        viewCommentLength(preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT",
            preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:ViewCommentLength",
            512);
        });

   /* 変更に従い、関連するチェックボックスを変更する */
    $("#" + preId + "\\:theBlockSectionDelivery\\:theBlockSectionItemDELIVERY_MANAGE\\:DELIVERY_MANAGE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionDelivery\\:theBlockSectionItemDELIVERY_STANDARD\\:DELIVERY_STANDARD", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD").change(function(e){
        if (! $(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_SETTINGS\\:WORKFLOW_SETTINGS").removeAttr("checked");
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_DEFINITION\\:WORKFLOW_DEFINITION").removeAttr("checked");
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_CATEGORY\\:WORKFLOW_CATEGORY").removeAttr("checked");
        }
        });
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_SETTINGS\\:WORKFLOW_SETTINGS").change(function(e){
        if ($(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD").attr("checked", "checked");
        }
        });
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_DEFINITION\\:WORKFLOW_DEFINITION").change(function(e){
        if ($(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD").attr("checked", "checked");
        }
        });
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_CATEGORY\\:WORKFLOW_CATEGORY").change(function(e){
        if ($(this).attr("checked")) {
            $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD").attr("checked", "checked");
        }
        });
    $("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_MANAGE\\:WORKFLOW_MANAGE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_STANDARD\\:WORKFLOW_STANDARD", $(this).attr("checked"));
        linkedCheckbox("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_SETTINGS\\:WORKFLOW_SETTINGS", $(this).attr("checked"));
        linkedCheckbox("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_DEFINITION\\:WORKFLOW_DEFINITION", $(this).attr("checked"));
        linkedCheckbox("#" + preId + "\\:theBlockSectionWorkflow\\:theBlockSectionItemWORKFLOW_CATEGORY\\:WORKFLOW_CATEGORY", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_MANAGE\\:COLLABO_SPACE_MANAGE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_ROOT_DEFINE\\:COLLABO_SPACE_ROOT_DEFINE", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_ROOT_DEFINE\\:COLLABO_SPACE_ROOT_DEFINE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionCollabo\\:theBlockSectionItemCOLLABO_SPACE_STANDARD\\:COLLABO_SPACE_STANDARD", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_MANAGE\\:COMPANY_SPACE_MANAGE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_ROOT_DEFINE\\:COMPANY_SPACE_ROOT_DEFINE", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_ROOT_DEFINE\\:COMPANY_SPACE_ROOT_DEFINE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionCompany\\:theBlockSectionItemCOMPANY_SPACE_STANDARD\\:COMPANY_SPACE_STANDARD", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionArchive\\:theBlockSectionItemARCHIVE_SPACE_MANAGE\\:ARCHIVE_SPACE_MANAGE").change(function(e){
        linkedCheckbox("#" + preId + "\\:theBlockSectionArchive\\:theBlockSectionItemARCHIVE_SPACE_STANDARD\\:ARCHIVE_SPACE_STANDARD", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionSupport\\:theBlockSectionItemSUPPORT_MANAGE\\:SUPPORT_MANAGE").change(function(e) {
        linkedCheckbox("#" + preId + "\\:theBlockSectionSupport\\:theBlockSectionItemSUPPORT_STANDARD\\:SUPPORT_STANDARD", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionUserManage\\:theBlockSectionItemUSER_USER_MANAGE\\:USER_USER_MANAGE").change(function(e) {
        linkedCheckbox("#" + preId + "\\:theBlockSectionUserManage\\:theBlockSectionItemEDIT_USER_ID\\:EDIT_USER_ID", $(this).attr("checked"));
        });
    $("#" + preId + "\\:theBlockSectionEvidence\\:theBlockSectionItemEVIDENCE_MANAGE\\:EVIDENCE_MANAGE").change(function(e) {
        linkedCheckbox("#" + preId + "\\:theBlockSectionEvidence\\:theBlockSectionItemEVIDENCE_VIEW\\:EVIDENCE_VIEW", $(this).attr("checked"));
