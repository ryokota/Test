    // 「チェックイン」連動項目
    $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCHECKIN\\:CHECKIN").click(function(){
        if($(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCHECKOUT\\:CHECKOUT").attr("checked","checked");
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemUP_DOC_OTHER_USER\\:UP_DOC_OTHER_USER").attr("checked","checked");
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemDL_DOC\\:DL_DOC").attr("checked","checked");
        }else{
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCHECKOUT\\:CHECKOUT").removeAttr("checked");
        }
