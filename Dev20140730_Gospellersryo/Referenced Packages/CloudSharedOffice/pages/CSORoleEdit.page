    // 「チェックアウト」連動項目
    $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCHECKOUT\\:CHECKOUT").click(function(){
        if($(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCHECKIN\\:CHECKIN").attr("checked","checked");
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemUP_DOC_OTHER_USER\\:UP_DOC_OTHER_USER").attr("checked","checked");
