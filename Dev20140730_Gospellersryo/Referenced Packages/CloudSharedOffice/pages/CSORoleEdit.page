    // 「プロパティ変更」連動項目
    $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemCNG_DOC_PROP\\:CNG_DOC_PROP").click(function(){
        if($(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemVIEW_DOC_PROP\\:VIEW_DOC_PROP").attr("checked","checked");
