            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemVIEW_INV_USER\\:VIEW_INV_USER").attr("checked","checked");
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCNG_INV_USER\\:CNG_INV_USER").attr("checked","checked");
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemDEL_INV_USER\\:DEL_INV_USER").attr("checked","checked");
        }else{
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemCNG_INV_USER\\:CNG_INV_USER").removeAttr("checked");
            $("#" + preId + "\\:theBlockSectionOther\\:theBlockSectionItemDEL_INV_USER\\:DEL_INV_USER").removeAttr("checked");
        }
    });

    // 「プロパティ参照」連動項目
    $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemVIEW_DOC_PROP\\:VIEW_DOC_PROP").click(function(){
        if(!$(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemCNG_DOC_PROP\\:CNG_DOC_PROP").removeAttr("checked");
            $("#" + preId + "\\:theBlockSectionDoc\\:theBlockSectionItemVIEW_DOC_PROP_OTHER_USER\\:VIEW_DOC_PROP_OTHER_USER").removeAttr("checked");
