<script type="text/javascript">

var preId = "CSORoleEditPage\\:theForm\\:theBlock";

//alert($(getId('{!$Component.CSORoleEditPage.theForm.theBlock.hidden_roleId}')).val());

$(function(){
    connectView();
});

/* 画面と連動する */
function connectView(){
    /* コメント文字数をチェックする */
    $("#" + preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT").keyup(function(e){
        viewCommentLength(preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT",
            preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:ViewCommentLength" ,
            512);
        });

    /* 初期状態を反映する */
    $("#" + preId + "\\:theBlockSectionInfo\\:theBlockSectionItemComment\\:theBlockSectionItemCommentItem\\:COMMENT").keyup();

    /* チェックボックスと連動する */
    $(".inv").each(function(){
        $(this).click(function(){
            if($(this).attr("checked")){
                $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemVIEW_INV_SP\\:VIEW_INV_SP").attr("checked","checked");
            };
        });
    });

    $(".subInv").each(function(){
        $(this).click(function(){
            if($(this).attr("checked")){
                $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemVIEW_SUB_SP\\:VIEW_SUB_SP").attr("checked","checked");
            };
        });
    });

    // 「招待スペースの閲覧」連動項目
    $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemVIEW_INV_SP\\:VIEW_INV_SP").click(function(){
        if(!$(this).attr("checked")){
/*
            $.each($(":checkbox"),function(n,ck){
                $(ck).attr("checked","");
            });
*/
            $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemVIEW_INV_SP\\:VIEW_INV_SP").attr("checked","checked");
        }
    });

    // 「招待スペースの編集」連動項目
    $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemCNG_INV_SP\\:CNG_INV_SP").click(function(){
        if($(this).attr("checked")){
            $("#" + preId + "\\:theBlockSectionSp\\:theBlockSectionItemVIEW_INV_SP\\:VIEW_INV_SP").attr("checked","checked");
        }
