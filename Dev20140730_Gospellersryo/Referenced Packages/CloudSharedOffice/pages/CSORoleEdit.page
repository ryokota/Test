/* 指定のチェックボックスを変更する */
function linkedCheckbox(id, isOn){
    if(isOn){
        $(id).attr("checked", "checked");
        $(id).attr("disabled", "disabled");
    }else{
        $(id).removeAttr("disabled");
    }
    $(id).change();
}

