        <errorConditionFormula>AND( 
ISPICKVAL(  SecondCOACategory__c, &apos;VII. Other Transactions / Activities / Contracts and Agreements&apos;), 
ISPICKVAL(  SecondCOASubCategory__c, &apos;N. Approval for Creation or Change of Master Files&apos;), 
OR( 
ISPICKVAL( SecondCOASubCategory2__c, &apos;4. Price Master File&apos;), 
ISPICKVAL(SecondCOASubCategory2__c, &apos;5. Rebate Master File&apos;), 
ISPICKVAL(SecondCOASubCategory2__c, &apos;6. Standard Cost of Sales&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>SecondCOASubCategory2__c</errorDisplayField>
        <errorMessage>This COA Category could not be selected in The New RFA system. Please use NotesDB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR24_ThirdCOACategoryValues</fullName>
        <active>true</active>
        <description>Option 4, 5, and 6 of  &quot;ThirdCOACategory__c&quot; picklist are not available if ThirdCOASubCategory is &quot;VII&quot; and ThirdCOASubCategory2 is &quot;N&quot;</description>
        <errorConditionFormula>AND( 
ISPICKVAL(  ThirdCOACategory__c , &apos;VII. Other Transactions / Activities / Contracts and Agreements&apos;), 
ISPICKVAL(  ThirdCOASubCategory__c , &apos;N. Approval for Creation or Change of Master Files&apos;), 
OR( 
ISPICKVAL( ThirdCOASubCategory2__c  , &apos;4. Price Master File&apos;), 
ISPICKVAL(ThirdCOASubCategory2__c , &apos;5. Rebate Master File&apos;), 
ISPICKVAL(ThirdCOASubCategory2__c , &apos;6. Standard Cost of Sales&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>ThirdCOASubCategory2__c</errorDisplayField>
        <errorMessage>This COA Category could not be selected in The New RFA system. Please use NotesDB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR25_AmountRangeForSmallProject</fullName>
        <active>true</active>
        <errorConditionFormula>/*
AND( RecordType.DeveloperName=&apos;RFA_SmallProjectRequest&apos;, RequestAmountFunctionalCurrency__c &gt; 25000000 )
