RequestAmountFunctionalCurrency__c  &gt;  50000000 )

  )</errorConditionFormula>
        <errorDisplayField>RequestAmountFunctionalCurrency__c</errorDisplayField>
        <errorMessage>Request Amount of Medium Project  &gt;  ¥25,000,000   and   ≦　¥50,000,000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR27_AmountRangeForCapitalProject</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName=&apos;RFA_CapitalProjectRequest&apos; , 
RequestAmountFunctionalCurrency__c &lt;= 50000000 )</errorConditionFormula>
        <errorDisplayField>RequestAmountFunctionalCurrency__c</errorDisplayField>
        <errorMessage>Request Amount of Capital Project ＞ ¥50,000,000</errorMessage>
    </validationRules>
