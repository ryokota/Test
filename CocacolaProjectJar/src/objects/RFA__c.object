        <errorConditionFormula>AND( 
ISPICKVAL(IsthisanAnnualCapitalPlanrequest__c, &apos;Yes&apos;), 
Budget_Approval_Number__c == null, 
OR( 
$RecordType.DeveloperName = &apos;RFA_SmallProjectRequest&apos;, 
$RecordType.DeveloperName = &apos;RFA_CapitalProjectRequest&apos;, 
$RecordType.DeveloperName = &apos;RFA_Small_Project_Request&apos; )
)</errorConditionFormula>
        <errorDisplayField>Budget_Approval_Number__c</errorDisplayField>
        <errorMessage>Please input Budget Approval Number when Annual Capital Plan request is True.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CokeOne_Travel_and_Enter_Cost_Center</fullName>
        <active>true</active>
        <description>CokeOne RFA can only select CokeOne Cost Center</description>
        <errorConditionFormula>IF(
  OR(
    $RecordType.DeveloperName = &apos;Travel_Expenses_Request&apos;   
    ,
    $RecordType.DeveloperName = &apos;Entertainment_Request&apos;
    ) 
 , 
 IF( Cost_Center_Number__r.Function_En__c != &apos;CokeOne&apos;, true, false) , 
 false
)</errorConditionFormula>
        <errorDisplayField>Cost_Center_Number__c</errorDisplayField>
        <errorMessage>Please select CokeOne Cost Center: C2800038000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR02_RequestAmountRequired</fullName>
        <active>true</active>
        <description>RFA R1 - Request Amount is a required field for an RFA for all stages apart from Draft.</description>
        <errorConditionFormula>AND( NOT( ISPICKVAL(Stage__c, &apos;Draft&apos;)), ISBLANK(RequestAmountFunctionalCurrency__c))</errorConditionFormula>
        <errorDisplayField>RequestAmountFunctionalCurrency__c</errorDisplayField>
        <errorMessage>Please enter the Request Amount for the Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR06_InitialBudgetYearRequired</fullName>
        <active>true</active>
        <description>RFA R1 - Initial Required field for putting the RFA into Circulation (i.e. not in Draft Stage) for RFA Type (Record Type) CPR, SPR, CAPPR and Annual Group Consolidated Plan.</description>
        <errorConditionFormula>AND( 
  NOT( ISPICKVAL(Stage__c, &apos;Draft&apos;)), 
  ISPICKVAL( InitialBudgetYear__c , &apos;&apos; ), 
  OR( 
   $RecordType.DeveloperName = &apos;RFA_SmallProjectRequest&apos;,   
   $RecordType.DeveloperName = &apos;RFA_Small_Project_Request&apos;,  
   $RecordType.DeveloperName = &apos;RFA_CapitalProjectRequest&apos; 
   )
)</errorConditionFormula>
        <errorDisplayField>InitialBudgetYear__c</errorDisplayField>
        <errorMessage>Please enter Budget Year for this RFA Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR07_CapitalPlanIndicatorRequired</fullName>
        <active>true</active>
        <description>RFA R1 - Annual Capital Plan request indicator field isRequired field for putting the RFA into Circulation for RFA Type (Record Type) CPR, SPR, CAPPR.</description>
        <errorConditionFormula>AND( 
  NOT( ISPICKVAL(Stage__c, &apos;Draft&apos;)), 
  ISPICKVAL( IsthisanAnnualCapitalPlanrequest__c , &apos;&apos; ), 
  OR( 
  $RecordType.DeveloperName = &apos;RFA_SmallProjectRequest&apos;,  
  $RecordType.DeveloperName = &apos;RFA_CapitalProjectRequest&apos;, 
  $RecordType.DeveloperName = &apos;RFA_Small_Project_Request&apos; )

)</errorConditionFormula>
        <errorDisplayField>IsthisanAnnualCapitalPlanrequest__c</errorDisplayField>
        <errorMessage>Please select Yes or No for this RFA Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR08_BudgetTypeRequired</fullName>
        <active>true</active>
        <description>RFA R1 - Budget Type is Required field for putting the RFA into Circulation (i.e. outside Draft Stage) for RFA Type (Record Type) CPR, SPR, CAPPR.</description>
        <errorConditionFormula>AND( 
  NOT( ISPICKVAL(Stage__c, &apos;Draft&apos;)), 
  ISPICKVAL( BudgetType__c , &apos;&apos; ), 
  OR( 
  $RecordType.DeveloperName = &apos;RFA_SmallProjectRequest&apos;,  
  $RecordType.DeveloperName = &apos;RFA_CapitalProjectRequest&apos;, 
  $RecordType.DeveloperName = &apos;RFA_Small_Project_Request&apos; 
  )
)</errorConditionFormula>
        <errorDisplayField>BudgetType__c</errorDisplayField>
        <errorMessage>Please select Budget Type for this RFA Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR15_UpdateCordAdmFields</fullName>
        <active>true</active>
        <description>RFA R1 - Users cannot modify any of the Coordinator Administration fields while the RFA is in the &quot;Return to Sender&quot; status</description>
        <errorConditionFormula>ISPICKVAL(Stage__c, &quot;Return to Requester&quot;) &amp;&amp; 
( ISCHANGED( CapitalManagementAdministered__c ) ||
 ISCHANGED( Ratification__c ) ||
 ISCHANGED( Comments__c ) ||
 ISCHANGED( IncludeInCapitalReports__c ) )</errorConditionFormula>
        <errorMessage>Coordinator Administration fields cannot be modified while the RFA is in Return to Sender.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR16_ReturnReasonMandatory</fullName>
        <active>true</active>
        <description>RFA R1 - Return Reason is Mandatory when RFA Level is selected as Return to Sender</description>
        <errorConditionFormula>AND( ISPICKVAL(Stage__c, &apos;Return to Requester&apos;), 
 ISBLANK(ReturnReason__c)
)</errorConditionFormula>
        <errorDisplayField>ReturnReason__c</errorDisplayField>
        <errorMessage>Please provide the reason for Return. Return Reason is required when selected level for RFA is Return To Sender</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR17_PositiveRequestAmount</fullName>
        <active>true</active>
        <description>Request Amount must be positive.</description>
        <errorConditionFormula>RequestAmountFunctionalCurrency__c &lt; 0</errorConditionFormula>
        <errorDisplayField>RequestAmountFunctionalCurrency__c</errorDisplayField>
        <errorMessage>Request Amount must be positive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR18_ProfitCenterChangeInCirculation</fullName>
        <active>true</active>
        <description>For 2013 Q1 FR1.79 - Prevent the user from changing the Profit Center on an RFA while the RFA is in Circulation</description>
        <errorConditionFormula>ISPICKVAL(Stage__c, &apos;Circulation&apos;) &amp;&amp; IsChanged(ProfitCenterNumber__c)</errorConditionFormula>
        <errorDisplayField>ProfitCenterNumber__c</errorDisplayField>
        <errorMessage>The Profit Center cannot be changed while the RFA is in a Circulation stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR19_CompanyParticipationAmount</fullName>
        <active>true</active>
        <description>The Company Participation Amount must equal the RFA Request Amount.</description>
        <errorConditionFormula>IF(ISBLANK(CompanyParticipationAmountFC__c), false, CompanyParticipationAmountFC__c  &lt;&gt; RequestAmountFunctionalCurrency__c)</errorConditionFormula>
        <errorDisplayField>CompanyParticipationAmountFC__c</errorDisplayField>
        <errorMessage>The Company Participation Amount must equal the RFA Request Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR20_CompanyBottlerParticipationPerc</fullName>
        <active>true</active>
        <description>The Company Participation (%) plus the Bottler Participation (%) must equal 100%.</description>
        <errorConditionFormula>IF(ISBLANK(BottlerParticipation__c) &amp;&amp; ISBLANK(CompanyParticipation__c), false, BottlerParticipation__c + CompanyParticipation__c &lt;&gt; 1)</errorConditionFormula>
        <errorDisplayField>CompanyParticipation__c</errorDisplayField>
        <errorMessage>The Company Participation (%) plus the Bottler Participation (%) must equal 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR21_CompanyBottlerAmountValues</fullName>
        <active>true</active>
        <description>Either the Bottler Participation Amount, the Bottler Participation (%) or the Company Participation (%) must be adjusted to ensure the Company/Bottler Participation %’s accurately represent the Company/Bottler Participation Amounts.</description>
        <errorConditionFormula>IF(ISBLANK( BottlerParticipationAmountFC__c ) &amp;&amp; ISBLANK( CompanyParticipationAmountFC__c ) &amp;&amp; ISBLANK( CompanyParticipation__c ) &amp;&amp; ISBLANK( BottlerParticipation__c ), false, ROUND((CompanyParticipationAmountFC__c / (BottlerParticipationAmountFC__c + CompanyParticipationAmountFC__c)), 2) &lt;&gt; CompanyParticipation__c )</errorConditionFormula>
        <errorDisplayField>BottlerParticipation__c</errorDisplayField>
        <errorMessage>Either the Bottler Participation Amount, the Bottler Participation (%) or the Company Participation (%) must be adjusted to ensure the Company/Bottler Participation %’s accurately represent the Company/Bottler Participation Amounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR22_PrimaryCOACategoryValues</fullName>
        <active>true</active>
        <description>Option 4, 5, and 6 of  &quot;PrimaryCOASubCategory2__c&quot; picklist are not available if PrimaryCOACategory is &quot;VII&quot; and PrimaryCOASubCategory is &quot;N&quot;</description>
        <errorConditionFormula>AND( 
ISPICKVAL( PrimaryCOACategory__c , &apos;VII. Other Transactions / Activities / Contracts and Agreements&apos;), 
ISPICKVAL( PrimaryCOASubCategory__c , &apos;N. Approval for Creation or Change of Master Files&apos;), 
OR( 
ISPICKVAL(PrimaryCOASubCategory2__c , &apos;4. Price Master File&apos;), 
ISPICKVAL(PrimaryCOASubCategory2__c , &apos;5. Rebate Master File&apos;), 
ISPICKVAL(PrimaryCOASubCategory2__c , &apos;6. Standard Cost of Sales&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>PrimaryCOASubCategory2__c</errorDisplayField>
        <errorMessage>This COA Category could not be selected in The New RFA system. Please use NotesDB.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_VR23_SecondCOACategoryValues</fullName>
