    <validationRules>
        <fullName>CM_PaymentAmount_Type09_Rule</fullName>
        <active>true</active>
        <description>09. Promotion / Other expense Payment Amount Input check.</description>
        <errorConditionFormula>And($RecordType.DeveloperName = &quot;CM_09_Promotion_OtherExpense&quot;,
Or(
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;Total amount per transaction ＞　10,000,000 yen&quot;), 
