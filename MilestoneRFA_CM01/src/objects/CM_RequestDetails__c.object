    <validationRules>
        <fullName>CM_Contract_date_contradiction_rule</fullName>
        <active>true</active>
        <description>Contract_date_contradiction_rule</description>
        <errorConditionFormula>CM_Contract_term_from__c &gt;  CM_Contract_term_to__c</errorConditionFormula>
        <errorDisplayField>CM_Contract_term_to__c</errorDisplayField>
        <errorMessage>There is an inconsistency in the contract term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_Delivery_date_contradiction_rule</fullName>
        <active>true</active>
        <description>Contract date contradiction rule.</description>
        <errorConditionFormula>CM_Delivery_term_from__c &gt; CM_Delivery_term_to__c</errorConditionFormula>
        <errorDisplayField>CM_Delivery_term_to__c</errorDisplayField>
        <errorMessage>There is an inconsistency in the delivery term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_NumberOfStand_Input_Rule</fullName>
        <active>true</active>
        <description>Input required check of stand</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;CM_a_Nonstandard_Equipment&apos;  ,
    IsNull(CM_Can_Stand__c) , 
	IsNull(CM_Cup_Stand__c) , 
	IsNull(CM_Cooler_Stand__c) , 
	IsNull(CM_Dispenser_Stand__c) , 
	IsNull(CM_Other_Stand__c )
)</errorConditionFormula>
        <errorMessage>You must enter the number of equipment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_OldAndNewClassification_Input_Rule</fullName>
        <active>true</active>
        <description>Select required check of Old and new classification</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;CM_c_Incidental_work&apos;  ,
NOT(OR( CM_New_Equipment__c, CM_Used_Equipment__c, CM_Existing_Equipment__c))
)</errorConditionFormula>
        <errorMessage>You must enter the old and new classification.</errorMessage>
    </validationRules>
