	And(isPickval(CM_RFA__r.CM_Only_in_the_case_of_Rebate__c,&quot;＞ 10,000,000 yen 　and 　≦ 30,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 10000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 30000000))),
	And(isPickval(CM_RFA__r.CM_Only_in_the_case_of_Rebate__c,&quot;＞ 5,000,000 yen 　and 　≦ 15,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 5000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 15000000))),
	And(isPickval(CM_RFA__r.CM_Only_in_the_case_of_Rebate__c,&quot;≦ 5,000,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 5000000)),
	And(isPickval(CM_RFA__r.CM_Only_in_the_case_of_Rebate__c,&quot;＞ 1,000,000 yen 　and 　≦ 10,000,000 yen&quot;),
