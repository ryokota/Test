	And(isPickval(CM_RFA__r.CM_Only_in_the_case_of_Rebate__c,&quot;≦ 1,000,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 1000000))
))</errorConditionFormula>
        <errorDisplayField>CM_Payment_amount_Tax_exclusive_price__c</errorDisplayField>
        <errorMessage>Payment amount must match with what you have selected for &quot;07. Only in the case of Rebate&quot; on the request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_PaymentAmount_Type08_Rule</fullName>
        <active>true</active>
        <description>08. Other support Payment Amount Input check.</description>
        <errorConditionFormula>And($RecordType.DeveloperName = &quot;CM_08_Other_support&quot;,
Or(
