	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;＞ 3,000,000 yen 　and 　≦ 10,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 3000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 10000000))),
/* add(S) */
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;＞ 1,000,000 yen 　and 　≦ 25,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 1000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 25000000))),
/* add(E) */
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;≦ 3,000,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 3000000)),
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;＞ 1,000,000 yen 　and 　≦ 10,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 1000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 10000000))),
/* add(S) */
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;＞ 100,000 yen 　and 　≦ 1,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 100000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 1000000))),
/* add(E) */
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;≦ 1,000,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 1000000)),
/* add(S) */
	And(isPickval(CM_RFA__r.CM_KeyMoney_Rebate_Sport_Other__c,&quot;≦ 100,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 100000))
/* add(E) */
))</errorConditionFormula>
        <errorDisplayField>CM_Payment_amount_Tax_exclusive_price__c</errorDisplayField>
