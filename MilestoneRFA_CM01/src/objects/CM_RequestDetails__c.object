		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 5000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 7500000))),
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;＞ 1,000,000 yen 　and 　≦ 5,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 1000000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 5000000))),
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;＞ 250,000 yen 　and 　≦ 1,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 250000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 1000000))),
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;≦ 250,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 250000)),
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;＞ 100,000 yen 　and 　≦ 1,000,000 yen&quot;),
		Not(And(CM_Payment_amount_Tax_exclusive_price__c &gt; 100000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 1000000))),
	And(isPickval(CM_RFA__r.CM_Promotion_OtherExpense__c,&quot;≦ 100,000 yen&quot;),
		Not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 100000))
))</errorConditionFormula>
        <errorDisplayField>CM_Payment_amount_Tax_exclusive_price__c</errorDisplayField>
        <errorMessage>Payment amount must match with what you have selected for &quot;09. Promotion / Other expense&quot; on the request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CM_PaymentAmount_Type10_Rule</fullName>
        <active>true</active>
        <description>10. Promotion (Cooperation / Sampling) Payment Amount Input check.</description>
        <errorConditionFormula>And($RecordType.DeveloperName = &quot;CM_10_Promotion_CooperationSample_Sampling&quot;, 
Or(
	And(isPickval(CM_RFA__r.CM_Promotion_CooperationSampling__c,&quot;Per deal ≦ 50 cases  and ≦ 100,000 yen&quot;), 
		or(
			and(not(isNull(CM_Number_of_SampleCase__c)),
				not(CM_Number_of_SampleCase__c &lt;= 50)),
			and(not(isNull(CM_Payment_amount_Tax_exclusive_price__c )),
				not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 100000))
		)
	),		
	And(isPickval(CM_RFA__r.CM_Promotion_CooperationSampling__c,&quot;Per deal ＞ 50 cases   or  ＞ 100,000 yen&quot;),
		or(
			and(not(isNull(CM_Number_of_SampleCase__c)),
				not(CM_Number_of_SampleCase__c &gt; 50)),
			and(not(isNull(CM_Payment_amount_Tax_exclusive_price__c)),
				not(CM_Payment_amount_Tax_exclusive_price__c &gt; 100000))
		)
	),
/* --- add --- */
	And(isPickval(CM_RFA__r.CM_Promotion_CooperationSampling__c,&quot;Per deal ＞ 10 cases and ＞ 20,000 yen&quot;),
		or(
			and(not(isNull(CM_Number_of_SampleCase__c)),
				not(CM_Number_of_SampleCase__c &gt; 10)),
			and(not(isNull(CM_Payment_amount_Tax_exclusive_price__c)),
				not(CM_Payment_amount_Tax_exclusive_price__c &gt; 20000))
		)
	),

	And(isPickval(CM_RFA__r.CM_Promotion_CooperationSampling__c,&quot;Per deal ≦ 10 cases and ≦ 20,000 yen&quot;),
		or(
			and(not(isNull(CM_Number_of_SampleCase__c)),
				not(CM_Number_of_SampleCase__c &lt;= 10)),
			and(not(isNull(CM_Payment_amount_Tax_exclusive_price__c)),
				not(CM_Payment_amount_Tax_exclusive_price__c &lt;= 20000))
		)
	),

	And(isPickval(CM_RFA__r.CM_Promotion_CooperationSampling__c,&quot;Per deal ＞ 10 cases and ≦ 50 cases, ＞ 20,000 yen and  ≦ 100,000 yen&quot;),
		or(
			and(not(isNull(CM_Number_of_SampleCase__c)),
				not(
					and(CM_Number_of_SampleCase__c &gt; 10, CM_Number_of_SampleCase__c &lt;= 50))),
			and(not(isNull(CM_Payment_amount_Tax_exclusive_price__c)),
				not(
					and(CM_Payment_amount_Tax_exclusive_price__c &gt; 20000, CM_Payment_amount_Tax_exclusive_price__c &lt;= 100000)))
		)
	)
/* --- add end --- */
)
)</errorConditionFormula>
        <errorMessage>Payment amount（tax-exclusive price） or Number of Sample Case must match with what you have selected for &quot;10. Promotion (Cooperation/Sampling) &quot; on the request.</errorMessage>
    </validationRules>
