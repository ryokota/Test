    <validationRules>
        <fullName>ApprovalOrder_01_NoChange_RFAOutOfDrAft</fullName>
        <active>false</active>
        <description>Could not change Approver Order when RFA is not in Draft or Return to Requester status. (RFA__c/Stage__c)</description>
        <errorConditionFormula>AND(
   AND(
      NOT( ISPICKVAL(RFA__r.Stage__c, &apos;Draft&apos;) ),
      NOT( ISPICKVAL(RFA__r.Stage__c, &apos;Return to Requester&apos;) ),
      NOT( ISCHANGED( RFAStage__c ) ) 
   )
   ,    
  ISCHANGED( ApprovalOrder__c )
)


/*
AND(
   AND(
      NOT( ISPICKVAL(RFA__r.Stage__c, &apos;Draft&apos;) ),
      NOT( ISPICKVAL(RFA__r.Stage__c, &apos;Return to Requester&apos;) ),
  PRIORVALUE(RFAStage__c) != &apos;Draft&apos;    
   )
   ,    
  ISCHANGED( ApprovalOrder__c )
)
*/</errorConditionFormula>
        <errorDisplayField>ApprovalOrder__c</errorDisplayField>
        <errorMessage>Could not change Approval Order!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approval_Order_Error</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.Id = &quot;01210000000QsJj&quot; 
&amp;&amp;  ApprovalOrder__c = 0  , true, false)</errorConditionFormula>
        <errorDisplayField>ApprovalOrder__c</errorDisplayField>
        <errorMessage>Please enter the approval order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_for_Approvers_from_COA</fullName>
        <active>true</active>
        <errorConditionFormula>IF( TEXT(Approver_Source__c) != null ,  ISCHANGED(UserName__c), false)</errorConditionFormula>
        <errorDisplayField>UserName__c</errorDisplayField>
        <errorMessage>Can&apos;t change the user of Approver from COA rules!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR01_CommentsMandatoryCondApp</fullName>
        <active>true</active>
        <description>RFA R1 - Approval Comments is required when Approval Recommendation provided as Conditionally Approved or Rejected</description>
        <errorConditionFormula>AND( $RecordType.DeveloperName = &apos;RUR_Approver&apos;,  
OR(
 ISPICKVAL(
   ApprovalRecommendation__c, 
  &apos;Conditionally Approved / 承認（条件付き）&apos;), 
 ISPICKVAL(
  ApprovalRecommendation__c, 
  &apos;Rejected / 否認&apos;) 
),  
ISBLANK(ApprovalComments__c) 
)

/*
AND( $RecordType.DeveloperName = &apos;RUR_Approver&apos;,  OR(ISPICKVAL(ApprovalRecommendation__c, &apos;Conditionally Approved&apos;), ISPICKVAL(ApprovalRecommendation__c, &apos;Rejected&apos;) ),  ISBLANK(ApprovalComments__c) )
*/</errorConditionFormula>
        <errorDisplayField>ApprovalComments__c</errorDisplayField>
        <errorMessage>Approval Comment is required when Approval Recommendation is selected as Conditionally Approved or Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR02_UniqueAgentSelection</fullName>
        <active>true</active>
        <description>RFA R1 - Check for unique selection of agents in the system</description>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK(Agent1__c)), OR(Agent1__c == Agent2__c, Agent1__c == Agent3__c)),
AND(NOT(ISBLANK(Agent2__c)), OR(Agent2__c == Agent1__c, Agent2__c == Agent3__c)), 
AND(NOT(ISBLANK(Agent3__c)), OR(Agent3__c == Agent1__c, Agent2__c == Agent3__c))
)</errorConditionFormula>
        <errorMessage>Same User can not be selected more than once as a Agent for the same Approver</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR03_ApproverCannotBeAgent</fullName>
        <active>true</active>
        <description>RFA R1 - Validation rule to restrict users for selecting the Approver as agent for itself</description>
        <errorConditionFormula>OR(UserName__c == Agent1__c, UserName__c == Agent2__c, UserName__c == Agent3__c)</errorConditionFormula>
        <errorMessage>Same User can not be selected as Approver and Agent for the Same record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR04_NoUpdRecom_in_Circul_Validatio</fullName>
        <active>false</active>
        <description>RFA R1 - Do not allow Approval Recommendation and Approval Comments to be populated unless the Approval Process lookup field is populated.

Need To Check !!! by Jia Hu on 2014/05/23</description>
        <errorConditionFormula>/*
ApprovalProcess__c == &quot;&quot;  &amp;&amp;  ApprovalComments__c != &quot;Info Copy Only&quot; &amp;&amp;
(NOT( ISBLANK( TEXT (ApprovalRecommendation__c ))) || NOT( ISBLANK(ApprovalComments__c )))
*/

ApprovalProcess__c == &quot;&quot;  &amp;&amp;  ApprovalComments__c != &quot;Info Copy Only&quot; &amp;&amp; ApprovalComments__c != &quot;Info Copy After Apply&quot; &amp;&amp;
(NOT( ISBLANK( TEXT (ApprovalRecommendation__c ))) || NOT( ISBLANK(ApprovalComments__c )))</errorConditionFormula>
        <errorMessage>The Approval Recommendation field and the Approval Comments field can only be populated once the approval record has been submitted to the Approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR05_AppandAgent_in_Circul_Validatio</fullName>
        <active>true</active>
        <description>RFA R1 - Approver and Agent references cannot be changed if Approval Recommendation has been received from the Original Actor or its associated Agent</description>
        <errorConditionFormula>NOT(ISPICKVAL( ApprovalRecommendation__c, &quot;Awaiting Approval&quot;)) &amp;&amp; 
NOT(ISPICKVAL( ApprovalRecommendation__c, &quot;&quot;)) 
&amp;&amp;
(
  (ISCHANGED(UserName__c) 
   || ISCHANGED(Agent1__c) 
   || ISCHANGED(Agent2__c) 
   || ISCHANGED(Agent3__c)
)
 &amp;&amp;
 (
   NOT(
     OR($Profile.Name = &quot;System Administrator&quot;,  
        $Profile.Name = &quot;システム管理者&quot; )
     )
   )
)

/*
NOT(ISPICKVAL( ApprovalRecommendation__c, &quot;Awaiting Approval&quot;)) &amp;&amp; 
NOT(ISPICKVAL( ApprovalRecommendation__c, &quot;&quot;)) &amp;&amp;
(ISCHANGED(UserName__c) || ISCHANGED(Agent1__c) || ISCHANGED(Agent2__c) || ISCHANGED(Agent3__c))
*/</errorConditionFormula>
        <errorMessage>Approver or Agent cannot be changed after Approval Recommendation has been received.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR06_Unblank_Approval_Recommendation</fullName>
        <active>false</active>
        <description>User cannot reset the approval recommendation while the RFA is in Circulation
Disabled by Jia Hu on 2013/12/09  Need TO Check!!</description>
        <errorConditionFormula>AND( 
ISBLANK(TEXT(ApprovalRecommendation__c)), 
NOT(ISBLANK(PRIORVALUE(ApprovalRecommendation__c))), 
ISPICKVAL( RFA__r.Stage__c, &quot;Circulation&quot;), 
OR( 
ISPICKVAL( ApprovalProcess__r.Status__c , &quot;In Progress&quot;), 
ISPICKVAL( ApprovalProcess__r.Status__c, &quot;Completed&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>ApprovalRecommendation__c</errorDisplayField>
        <errorMessage>Approval Recommendation must be populated while the RFA is in Circulation. Select a valid Approval Recommendation or Cancel to return to the previous page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR07_NoUpdApprover_in_Circulation</fullName>
        <active>true</active>
        <description>RFA R1 - Do not allow User Name to be updated unless the Approval Process lookup field is not populated.</description>
        <errorConditionFormula>NOT( ISBLANK ( TEXT (ApprovalRecommendation__c) )) &amp;&amp; ISCHANGED(  UserName__c )</errorConditionFormula>
        <errorDisplayField>UserName__c</errorDisplayField>
        <errorMessage>The User Name field cannot be changed once the approval record has been submitted to the Approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR09_Lock_RecordType</fullName>
        <active>true</active>
        <description>Cannot change record type value</description>
        <errorConditionFormula>ISCHANGED( RecordTypeId )</errorConditionFormula>
        <errorMessage>Cannot Change Record Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR10_Lock_ApproverOrder</fullName>
        <active>true</active>
        <description>The approver order of &quot;Maniwa Daisuke &quot; could not be modified.</description>
        <errorConditionFormula>AND(
OR(
RFA__r.RecordType.DeveloperName=&apos;RFA_SmallProjectRequest&apos;,
RFA__r.RecordType.DeveloperName=&apos;RFA_CapitalProjectRequest&apos;,
RFA__r.RecordType.DeveloperName=&apos;RFA_Small_Project_Request&apos;
),
ISCHANGED( ApprovalOrder__c ),
UserName__r.CommunityNickname=&apos;daisuke.maniwa&apos;

)</errorConditionFormula>
        <errorDisplayField>ApprovalOrder__c</errorDisplayField>
        <errorMessage>This order could not be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUR_VR_CommentsMandatoryCondApp</fullName>
        <active>true</active>
        <description>RFA R1 - Approval Comments is required when Approval Recommendation provided as Unconditionally Approved</description>
        <errorConditionFormula>AND( 
$RecordType.DeveloperName = &apos;RUR_Approver&apos;,  
ISPICKVAL(ApprovalRecommendation__c, &apos;Unconditionally Approved / 承認（条件なし）&apos;),  
!ISBLANK(ApprovalComments__c) 
)


/*
AND( 
$RecordType.DeveloperName = &apos;RUR_Approver&apos;,  
OR(ISPICKVAL(ApprovalRecommendation__c, &apos;Unconditionally Approved&apos;), ISPICKVAL(ApprovalRecommendation__c, &apos;承認（条件なし）&apos;)),  
!ISBLANK(ApprovalComments__c) 
)*/</errorConditionFormula>
        <errorDisplayField>ApprovalComments__c</errorDisplayField>
        <errorMessage>Approval Comment is NOT required when Approval Recommendation is selected as Unconditionally Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requester_Same_To_Approver</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $RecordType.DeveloperName = &apos;RUR_Approver&apos;,
   UserName__c =  RFA__r.CreatedById 
)</errorConditionFormula>
        <errorDisplayField>UserName__c</errorDisplayField>
        <errorMessage>Requester can&apos;t be the approver.</errorMessage>
    </validationRules>
</CustomObject>
